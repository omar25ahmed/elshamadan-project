<!-- ====== Contact Section Start -->
  <% flash.each do |type, msg| %>
  <div>
    <div id="alert-border-1" class="flex p-4 mb-4 text-blue-800 border-t-4 border-blue-300 bg-blue-50 dark:text-blue-400 dark:bg-gray-800 dark:border-blue-800" role="alert">
    <svg class="flex-shrink-0 w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-7-4a1 1 0 11-2 0 1 1 0 012 0zM9 9a1 1 0 000 2v3a1 1 0 001 1h1a1 1 0 100-2v-3a1 1 0 00-1-1H9z" clip-rule="evenodd"></path></svg>
    <div class="ml-3 text-sm font-medium">
      <%= msg %>
    </div>
    <button type="button" class="ml-auto -mx-1.5 -my-1.5 bg-blue-50 text-blue-500 rounded-lg focus:ring-2 focus:ring-blue-400 p-1.5 hover:bg-blue-200 inline-flex h-8 w-8 dark:bg-gray-800 dark:text-blue-400 dark:hover:bg-gray-700" data-dismiss-target="#alert-border-1" aria-label="Close">
      <span class="sr-only">Dismiss</span>
      <svg aria-hidden="true" class="w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg>
    </button>
  </div>
<% end %>
<section class="bg-[#244f79] py-20 lg:py-[120px] overflow-hidden relative z-10 min-h-auto">
   <div class="">
      <div class="flex flex-wrap lg:justify-center m-auto items-center">
         <div class="w-full lg:w-1/2 xl:w-6/12 px-4">
            <div class="max-w-[570px] mb-12 lg:mb-0">
               <span class="block mb-4 text-base text-[#FF9925] font-semibold">
               Contact Us
               </span>
               <h2
                  class="
                  text-white
                  mb-6
                  uppercase
                  font-bold
                  text-[32px]
                  sm:text-[40px]
                  lg:text-[36px]
                  xl:text-[40px]
                  "
                  >
                  GET IN TOUCH WITH US
               </h2>
               <p class="text-base text-white leading-relaxed">
                  Thank you for your interest in our company. We would love to hear from you and answer any questions you may have. Please feel free to get in touch with us using the contact form or the information provided below.
               </p>

               <%= image_tag "contact/form.svg", class: "" %>

            </div>
         </div>
         <div class="w-full lg:w-1/2 xl:w-4/12 px-4 h-full">
            <div class="bg-[#111827] relative rounded-lg p-8 sm:p-12 shadow-lg">
               <%= form_with url: contact_index_path, method: :post, local: true do |f| %>
                  <div class="mb-2">
                     <%= label_tag :name, "Your Name", class: "block text-sm md:text-base font-medium text-white mb-2" %>
                     <%= f.text_field :name, placeholder: "John Doe", class: "w-full bg-[#244f79] rounded py-3 px-[14px] text-white text-base border border-[f0f0f0] outline-none focus-visible:shadow-none focus:border-primary placeholder:text-gray-400", required: true %>
                  </div>
                  <div class="mb-2">
                     <%= label_tag :email, "Your Email", class: "block text-sm md:text-base font-medium text-white mb-2" %>
                     <%= f.email_field :email, placeholder: "johndoe@example.com", class: "w-full bg-[#244f79] rounded py-3 px-[14px] text-white text-base border border-[f0f0f0] outline-none focus-visible:shadow-none focus:border-primary placeholder:text-gray-400", required: true %>
                  </div>
                  <div class="mb-2">
                     <%= label_tag :phone, "Your Phone", class: "block text-sm md:text-base font-medium text-white mb-2" %>
                     <%= f.telephone_field :phone, placeholder: "12345678910", class: "w-full bg-[#244f79] rounded py-3 px-[14px] text-white text-base border border-[f0f0f0] outline-none focus-visible:shadow-none focus:border-primary placeholder:text-gray-400", required: true %>
                  </div>
                  <div class="mb-6">
                     <%= label_tag :message, "Your Message", class: "block text-sm md:text-base font-medium text-white mb-2" %>
                     <%= f.text_area :message, rows: 6, placeholder: "Write anything you want to tell us...", class: "w-full bg-[#244f79] rounded py-3 px-[14px] text-white text-base border border-[f0f0f0] resize-none outline-none focus-visible:shadow-none focus:border-primary placeholder:text-gray-400", required: true %>
                  </div>
                  <div>
                     <%= f.submit "Send Message", class: "w-full text-white bg-[#FF9925] rounded p-3 transition hover:bg-[#DD5121] cursor-pointer" %>
                  </div>
               <% end %>
